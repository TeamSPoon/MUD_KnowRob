#-----------------------------------------------------------------------------------
# subproperties for XDS documents

xdsm:healthcareFacilityType
	a owl:ObjectProperty;
	rdfs:domain doc:Document;
	rdfs:range heca:Organization.

xdsm:sourcePatientInfo
	a owl:ObjectProperty;
	rdfs:domain doc:Document;
	rdfs:range heca:Patient.

